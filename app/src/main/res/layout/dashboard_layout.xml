<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/linear_top"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/appBackground"
        android:orientation="vertical"
       >

        <FrameLayout
            android:layout_width="match_parent"

            android:layout_height="match_parent"
            android:background="@color/appBackground">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/landingpage_image_height">


                <com.daimajia.slider.library.SliderLayout
                    android:id="@+id/dashboard_slider"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"></com.daimajia.slider.library.SliderLayout>

                <LinearLayout
                    android:id="@+id/weather_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <TextView
                        android:id="@+id/temperature_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"


                        android:textColor="@color/white"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/condition"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"


                        android:textColor="@color/white"

                        />

                    <TextView
                        android:id="@+id/place"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"


                        android:textColor="@color/white"
                        android:textStyle="bold" />

                </LinearLayout>


            </FrameLayout>


            <ScrollView
                android:layout_width="match_parent"

                android:layout_height="match_parent"
                android:fastScrollEnabled="true">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:weightSum="1">


                    <LinearLayout
                        android:id="@+id/linearapt"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/landingpage_image_height"
                        android:layout_weight="0.01"
                        android:orientation="horizontal">

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linear_weather"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/appBackground">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/degree_id"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5dp"
                                android:text="23 c"
                                android:textColor="@color/black"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/condition_id"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5dp"
                                android:text="Rain,"
                                android:textColor="@color/black"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/place_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Kathmandu"
                                android:textColor="@color/black"
                                android:textStyle="bold" />

                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:background="@color/appBackground"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:weightSum="100">


                        <LinearLayout
                            android:id="@+id/booknow"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"

                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingLeft="@dimen/padding_icon_landingpage"
                            android:paddingRight="@dimen/padding_icon_landingpage">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">


                                <ImageView
                                    android:id="@+id/imageView3"
                                    android:layout_width="@dimen/icon_size_landingpage"
                                    android:layout_height="@dimen/icon_size_landingpage"
                                    android:src="@drawable/bookicon" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/gallery_hihight"></LinearLayout>


                            </FrameLayout>


                            <TextView
                                android:id="@+id/booknow_text"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:text="Book now"
                                android:textColor="@color/black" />


                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/linear_facilities"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingLeft="@dimen/padding_icon_landingpage"
                            android:paddingRight="@dimen/padding_icon_landingpage">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/icon_size_landingpage"
                                    android:layout_height="@dimen/icon_size_landingpage"
                                    android:src="@drawable/facilitiesicon" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/gallery_hihight"></LinearLayout>


                            </FrameLayout>

                            <TextView
                                android:id="@+id/facility_text"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:text="Facilities"
                                android:textColor="@color/black" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linear_contact"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingLeft="@dimen/padding_icon_landingpage"
                            android:paddingRight="@dimen/padding_icon_landingpage">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/icon_size_landingpage"
                                    android:layout_height="@dimen/icon_size_landingpage"
                                    android:src="@drawable/contacticon" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/gallery_hihight"></LinearLayout>


                            </FrameLayout>

                            <TextView
                                android:id="@+id/contact_text"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:text="Contact"
                                android:textColor="@color/black" />


                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/appBackground"
                        android:orientation="vertical">


                        <LinearLayout
                            android:id="@+id/apartment_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">


                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:id="@+id/myImageView"
                                    android:layout_width="match_parent"


                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:scaleType="centerCrop"

                                    android:src="@drawable/apartmentpic" />


                                <LinearLayout

                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>


                                <TextView
                                    android:layout_width="wrap_content"

                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="Apartments"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />


                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/aboutus_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">


                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">


                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@color/transparent"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/apartment" />


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="About Us"
                                    android:textAllCaps="true"

                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />

                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/location_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:scaleType="centerCrop"

                                    android:src="@drawable/map" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>


                                <TextView
                                    android:layout_width="wrap_content"

                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="Location"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />

                            </FrameLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/travel_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">


                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:scaleType="centerCrop"

                                    android:src="@drawable/swyambhu1" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="Travels"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />


                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/gallery_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:scaleType="centerCrop"

                                    android:src="@drawable/gallerypic" />


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>

                                <TextView
                                    android:layout_width="wrap_content"

                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="Gallery"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />
                            </FrameLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/testimonial_id"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/image_icon_height"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.example.prakash.RetreatServicedApartment.activity.RoundRectCornerImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:scaleType="centerCrop"

                                    android:src="@drawable/test" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/margin_landingpage_buttom"
                                    android:layout_marginLeft="@dimen/margin_on_landingpage"
                                    android:layout_marginRight="@dimen/margin_on_landingpage"
                                    android:layout_marginTop="@dimen/margin_landingpage_buttom"
                                    android:background="@drawable/landingpageradious"></LinearLayout>

                                <TextView
                                    android:layout_width="wrap_content"

                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:layout_margin="@dimen/landing_text_margin"
                                    android:text="Testimonials"
                                    android:textAllCaps="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/textsize_landingpage"
                                    android:textStyle="bold" />
                            </FrameLayout>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>

            </ScrollView>
        </FrameLayout>

    </LinearLayout>


</LinearLayout>